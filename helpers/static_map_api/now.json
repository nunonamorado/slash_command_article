{
    "version": 2,
    "alias": ["surforecaster-mapsproxy.now.sh"],
    "name": "StaticMapsProxyApi",
    "builds": [
        { "src": "index.js", "use": "@now/node-server" }
    ],
    "routes": [
        { "src": "/(.*)", "dest": "index.js" }
    ],
    "env": {
      "MAPBOX_API_TOKEN": "@mapbox-api-token"
    }
}
